{
	"name": "javascript-development-environment",
	"version": "1.0.0",
	"description": "JavaScript development environment",
	"scripts": {
		"prestart": "node buildScripts/message.js",
		"start": "npm-run-all test open",
		"open": "node buildScripts/srcServer.js",
		"localTunnel": "lt -t 3000",
		"share": "npm-run-all --parallel start open localTunnel",
		"test": "mocha --reporter progress \"src/**/*test.js\"",
		"test:watch": "npm run test -- --watch",
		"generate-mock-data": "node buildScripts/generateMockData.js"
	},
	"author": "Katleho Khanye",
	"license": "MIT",
	"dependencies": {
		"whatwg-fetch": "1.0.0"
	},
	"devDependencies": {
		"chai": "3.5.0",
		"chalk": "1.1.3",
		"cheerio": "0.22.0",
		"compression": "^1.7.4",
		"express": "^4.17.1",
		"extract-text-webpack-plugin": "3.0.2",
		"faker": "^4.1.0",
		"jsdom": "9.8.0",
		"json-schema-faker": "^0.5.0-rc17",
		"json-server": "^0.15.0",
		"localtunnel": "^1.9.2",
		"mocha": "^6.2.0",
		"nock": "^10.0.6",
		"npm-run-all": "3.1.1",
		"numeral": "1.5.3",
		"open": "6.0.0",
		"rimraf": "2.5.4",
		"style-loader": "0.13.2",
		"surge": "0.20.4",
		"typescript": "^3.5.3",
		"webpack": "^4.39.1",
		"webpack-dev-middleware": "1.8.4",
		"webpack-hot-middleware": "2.13.0",
		"webpack-md5-hash": "0.0.5"
	}
